                                    "Tile": {
                                        "Query": "Type=ITPC_CTX_Session_CL | distinct UserName_s",
                                        "Legend": "Unique User"
                                    },
                                    "List": {
                                        "Query": "Type=ITPC_CTX_PerfData_CL  | measure countdistinct(UserName_s) as Count by DesktopGroup_s",
